{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.{css,scss,sass,js,ts,tsx,jsx,json,ico,svg,png,jpg,jpeg,gif,webp,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
